[ [ fail ] [ not ] if ] :assert define

[ map eval unwrap ] :fold define

[ print "\n" print ] :println define

[
	[
		# on stack: symbol
		dup          # => symbol symbol
		to_list      # => symbol [ word ]
		swap wrap    # => [ word ] [ symbol ]
		swap prepend # => [ [ word ] symbol ]
	]
		map
		unwrap
]
:export
	define

[ :assert :fold :println :export ]
	export
